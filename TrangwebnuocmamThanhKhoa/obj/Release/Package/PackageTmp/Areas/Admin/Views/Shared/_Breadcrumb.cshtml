@model List<TrangwebnuocmamThanhKhoa.Areas.Admin.Data.BreadcrumbItem>

<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        @foreach (var item in Model)
        {
            if (item.IsActive)
            {
                <li class="breadcrumb-item active" aria-current="page">@item.NamePage</li>
            }
            else
            {
                <li class="breadcrumb-item"><a href="@item.Url">@item.NamePage</a></li>
            }
        }
    </ol>
</nav>
